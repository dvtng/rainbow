{
    "parser": "babel-eslint",
    "extends": [
        "airbnb",
        "prettier"
    ],
    "plugins": [
        "prettier"
    ],
    "rules": {
        "indent": [
            "error",
            4
        ],
        "react/prop-types": 0,
        "react/jsx-filename-extension": 0,
        "react/jsx-indent": [
            "error",
            4
        ],
        "react/jsx-wrap-multilines": 0,
        "import/no-named-as-default": 0
    },
    "globals": {
        "document": true,
        "window": true
    }
}